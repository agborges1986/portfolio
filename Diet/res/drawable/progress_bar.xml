<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background">
		<shape>
			<gradient android:startColor="#FF6D6D6D" android:angle="90"
				android:centerColor="#FFA1A1A1" android:endColor="#FF6D6D6D" />
			<stroke android:width="1dp" android:color="#FFBBBBBB" />
			<corners android:radius="4dp" />
		</shape>
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<inset android:insetTop="4dp" android:insetRight="4dp"
				android:insetBottom="4dp" android:insetLeft="4dp">
				<shape>
					<solid android:color="#FFAAAAAA" />
					<corners android:radius="2dp" />
				</shape>
			</inset>
		</clip>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<inset android:insetTop="4dp" android:insetRight="4dp"
				android:insetBottom="4dp" android:insetLeft="4dp">
				<shape>
					<solid android:color="#FFEEEEEE" />
					<corners android:radius="2dp" />
				</shape>
			</inset>
		</clip>
	</item>

</layer-list>