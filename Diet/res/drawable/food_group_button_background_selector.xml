<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_pressed="true">
		<inset android:insetTop="1dp" android:insetLeft="1dp"
			android:insetBottom="-1dp" android:insetRight="-1dp">
			<shape>
				<solid android:color="@color/cff"></solid>
				<stroke android:width="1dp" android:color="@color/c82" />
				<corners android:radius="8dp"/>
			</shape>
		</inset>		
	</item>
	<item>
		<shape>
			<solid android:color="@color/cff"></solid>
			<stroke android:width="1dp" android:color="@color/c82" />
			<corners android:radius="8dp"/>
		</shape>
	</item>

</selector>
