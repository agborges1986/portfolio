<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_enabled="false">
		<shape>
			<solid android:color="#848484" />
			<stroke android:color="#707070" android:width="1dp"/>			
		</shape>
	</item>
	<item android:state_pressed="true">
		<shape>
			<gradient android:startColor="#575757" android:endColor="#BABABA" android:angle="270"/>			
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#575757" android:endColor="#BABABA" android:angle="90"/>			
		</shape>
	</item>

</selector>

