		style="@style/ExpandableListChildItemStyle" android:layout_width="fill_parent"
		android:orientation="vertical" android:paddingLeft="2dp" android:paddingRight="2dp">
		<TextView android:text="TextView" android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="wrap_content" android:id="@+id/textView1"
			android:layout_height="wrap_content" android:textColor="@color/black"></TextView>
		<TextView android:id="@+id/textView2" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="x"
			android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
			android:textColor="@color/black"></TextView>
	</LinearLayout>
</LinearLayout>
