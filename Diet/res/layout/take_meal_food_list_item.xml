<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:paddingLeft="4dp"
	android:paddingRight="4dp">
	<TextView android:layout_height="wrap_content" android:text="TextView"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:id="@+id/textView1" android:layout_width="fill_parent"></TextView>
	<LinearLayout android:orientation="horizontal"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TextView android:layout_height="wrap_content" android:text="TextView"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/textView2" android:paddingLeft="10dp"
			android:layout_width="wrap_content"></TextView>
		<TextView android:text="TextView" android:id="@+id/textView3"
			android:layout_height="wrap_content" android:layout_width="fill_parent" android:gravity="right" android:paddingRight="20dp"></TextView>
	</LinearLayout>
</LinearLayout>
